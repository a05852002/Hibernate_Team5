<?xml version="1.0" encoding="UTF-8"?>


<hibernate-mapping>
    <class name="tw.hibernatedemo.model.myHouseBean" table="myHouse">
        <id name="houseid" type="java.lang.Integer">
            <column name="houseid" />
            <generator class="assigned" />
        </id>
        <property name="houseName" type="java.lang.String">
            <column name="houseName" sql-type="nvarchar(50)" />
        </property>
    </class>
</hibernate-mapping>