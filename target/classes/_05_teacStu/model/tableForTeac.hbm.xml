<?xml version="1.0" encoding="UTF-8"?>


<hibernate-mapping>
    <class name="_05_teacStu.model.tableForTeac" table="teacher">
        <id name="teacno" type="java.lang.Integer">
            <column name="teacno" />
            <generator class="assigned" />
        </id>
        <property name="memberid" type="java.lang.Integer">
            <column name="memberid" sql-type="Integer" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="title" sql-type="nvarchar(100)" />
        </property>
        <property name="postdate" type="java.lang.Date">
            <column name="postdate" sql-type="datetime2(0)" />
        </property>
        <property name="detail" type="java.lang.String">
            <column name="detail" sql-type="nvarchar(100)" />
        </property>
        <property name="price" type="java.lang.Double">
            <column name="price" sql-type="decimal(8,2)" />
        </property>
        <property name="subjectitem" type="java.lang.String">
            <column name="subjectitem" sql-type="nvarchar(100)" />
        </property>
        <property name="classpicture" type="java.lang.Blob">
            <column name="classpicture" sql-type="varbinary(max)" />
        </property>
    </class>
</hibernate-mapping>